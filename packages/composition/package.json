{
  "name": "@rental-storage-project/api-client",
  "scripts": {
    "build": "rimraf dist && tsc -b tsconfig.build.json",
    "build:test": "tsc -b tsconfig.test.json",
    "build:docker": "tsc -b tsconfig.docker.json",
    "deploy": "sls deploy --config serverless.yml --stage local",
    "remove": "sls remove --config serverless.yml --stage local",
    "generate:env": "sls export-env --stage local",
    "test:integration": "jest --coverage false"
  },
  "dependencies": {
    "axios": "1.1.3",
    "dotenv": "16.0.3",
    "glob": "8.0.3",
    "keyv": "4.5.2",
    "rimraf": "3.0.2",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@types/aws-lambda": "8.10.110",
    "@types/axios": "0.14.0",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "inquirer": "5.0.0",
    "jest": "29.2.1",
    "serverless": "2.72.3",
    "serverless-deployment-bucket": "1.6.0",
    "serverless-export-env": "2.2.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
